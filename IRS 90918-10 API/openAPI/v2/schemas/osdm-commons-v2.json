					"type": "string"
				},
				"travelDate": {
					"$ref": "osdm-basics-schema-v2.json#/definitions/DateTimeDef",
					"description": "date and time of the departure where the train link starts"
				}
			},
			"required": [
				"fromStation",
				"toStation",
				"train",
				"travelDate"
			],
			"patternProperties": {
				"^P_": {
					"anyOf": [
						{
							"type": "object"
						},
						{
							"type": "string"
						}
					]
				}
			},
			"additionalProperties": false
		},
		"TranslationDef": {
			"type": "object",
