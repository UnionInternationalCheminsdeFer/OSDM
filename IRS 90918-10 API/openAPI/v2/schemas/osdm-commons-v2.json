		"TripDef": {
			"type": "object",
			"properties": {
				"segments": {
					"type": "array",
					"items": {
						"type": "object",
						"properties": {
							"segment": {
								"type": "object",
								"properties": {
									"seqId": {
										"description": "ordering of the trains",
										"type": "number"
									},
									"transfer": {
										"description": "Transfer to the next vehicle not included in the request",
										"type": "object",
										"properties": {
											"duration": {
												"description": "duration forseen as transfer to the next section",
												"type": "number"
											},
											"transferType": {
