						"$ref": "#/definitions/TranslationDef"
					}
				},
				"text": {
					"description": "text without special characters according to ICAO specification",
					"type": "string"
				},
				"shortTextUtf8": {
					"type": "string"
				},
				"shortText": {
					"description": "text without special characters according to ICAO specification",
					"type": "string"
				}
			},
			"required": [
				"id",
				"textUTF8",
				"text"
			],
			"additionalProperties": false
		},
		"TrainLinkDef": {
			"type": "object",
			"properties": {
				"fromStation": {
					"$ref": "osdm-basics-schema-v2.json#/definitions/StationDef"
				},
				"toStation": {
					"$ref": "osdm-basics-schema-v2.json#/definitions/StationDef"
